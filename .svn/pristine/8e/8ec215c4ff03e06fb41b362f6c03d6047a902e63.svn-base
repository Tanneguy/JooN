package net.nooj4nlp.engine.helper;

import java.io.Serializable;

@SuppressWarnings({ "hiding", "serial" })
public class PenAttributes<Color, Float> implements Serializable
{
	public Color color;
	// not using BasicStroke, as it's not serializable
	public float stroke;

	public PenAttributes(Color c, float s)
	{
		color = c;
		stroke = s;
	}
}