package net.nooj4nlp.engine;

public class UnsignedByte implements java.io.Serializable {

	private byte b;
	
	public UnsignedByte() {
		super();
	}

	public UnsignedByte(byte b) {
		super();
		this.b = b;
	}

	public static final int MAX_VALUE = 255;
	
	public int getInt() {
		if (b > 0)
			return (int) b;
		else
			return (int)(b & 0xFF);
	}

	public void setB(byte b) {
		this.b = b;
	}
}
