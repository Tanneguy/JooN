package net.nooj4nlp.engine;

/**
 * Helper class - used for keeping pairs of integers (destination, label).
 * 
 * @author Silberztein Max
 */
class Dstlbl
{
	int dst;
	int lbl;

	/**
	 * Constructor
	 * 
	 * @param dstnb
	 * @param idlabel
	 */
	Dstlbl(int dstnb, int idlabel)
	{
		this.dst = dstnb;
		this.lbl = idlabel;
	}
}